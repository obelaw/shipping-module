{
    "name": "obelaw/shipping",
    "description": "A comprehensive shipping module, enabling functionalities like carrier integration, rate calculation, label printing, and shipment tracking.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Obelaw\\Shipping\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obelaw\\Shipping\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Karim Mohamed",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "twist/twist": "^4.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "orchestra/testbench": "^9.14",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obelaw\\Shipping\\ObelawShippingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
